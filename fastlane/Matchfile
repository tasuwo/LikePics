# vim: syntax=ruby

git_url("git@github.com:tasuwo/certificates.git")
storage_mode("git")

type("development")
app_identifier([
    "net.tasuwo.TBox",
    "net.tasuwo.TBox.dev",
    "net.tasuwo.TBox.ShareExtension",
    "net.tasuwo.TBox.dev.ShareExtension"
])

for_lane :release do
  type("appstore")
  readonly(true)
  app_identifier([
    "net.tasuwo.TBox",
    "net.tasuwo.TBox.ShareExtension"
  ])
end

