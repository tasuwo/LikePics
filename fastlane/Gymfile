# vim: syntax=ruby

scheme("LikePics")
sdk("iphoneos")
output_directory("./build")
include_bitcode(false)
clean(true)

for_lane :release do
  configuration("Release")
  export_method("app-store")
  include_bitcode(true)

  output_name("likepics.ipa")
end

for_lane :release_build_only do
  configuration("Release")
  export_method("app-store")
  include_bitcode(true)

  output_name("likepics.ipa")
end

